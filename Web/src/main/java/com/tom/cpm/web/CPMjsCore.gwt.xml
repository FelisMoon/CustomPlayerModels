<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.9.0//EN"
  "http://gwtproject.org/doctype/2.9.0/gwt-module.dtd">
<module rename-to='cpmjs'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  
  <inherits name='com.tom.cpl.CPL'/>
  <inherits name='com.tom.cpm.api.CPM'/>
  <inherits name='com.tom.cpm.shared.CPM'/>
  <inherits name='com.tom.cpm.externals.CPM'/>
  <inherits name='com.tom.ugwt.UGWT'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <inherits name='elemental2.core.Core'/>
  <inherits name='elemental2.webgl.WebGl'/>
  <inherits name='elemental2.promise.Promise'/>
  <inherits name='elemental2.dom.Dom'/>
  <inherits name="com.google.common.collect.Collect"/>
  <inherits name="com.google.common.cache.Cache"/>
  <inherits name="com.google.common.io.Io"/>
  <inherits name='com.ekotrope.gwt.CompletableFuture'/>
  <inherits name='elemental2.webstorage.WebStorage'/>
  
  <define-configuration-property name="cpm.debug" is-multi-valued='false' />
  <define-configuration-property name="cpm.version" is-multi-valued='false' />
  <define-configuration-property name="cpm.webApiEndpoint" is-multi-valued='false' />
  
  <set-property name="user.agent" value="safari, gecko1_8"/>
  <collapse-all-properties/>
  <set-property name="compiler.useSourceMaps" value="true"/>
  <set-configuration-property name="includeSourceMapUrl" value="true"/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
</module>
