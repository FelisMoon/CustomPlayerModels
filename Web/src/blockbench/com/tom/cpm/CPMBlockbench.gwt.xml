<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.9.0//EN"
  "http://gwtproject.org/doctype/2.9.0/gwt-module.dtd">
<module rename-to='cpmblockbench'>
  <inherits name='com.tom.cpm.web.CPMjsCore'/>
  <inherits name='com.tom.ugwt.UGWT'/>
  
  <define-configuration-property name="cpm.pluginId" is-multi-valued='false' />

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.tom.cpm.blockbench.PluginStart'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='blockbench'/>

  <add-linker name="ugwt"/>
</module>
